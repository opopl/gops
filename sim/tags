1	bln.f90	/^     1        dot_prod(i-1,3)*dot_prod(i,1))*(-dot_prod(i+1,1)*zr(i,i+1) +$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,3)*dot_prod(i,1))*(dot_prod(i,1)*xr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,3)*dot_prod(i,1))*(dot_prod(i,1)*yr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,3)*dot_prod(i,1))*(dot_prod(i,1)*zr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,1)*xr(i-3,i-2) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,1)*yr(i-3,i-2) $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,1)*yr(i-3,i-2) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,1)*zr(i-3,i-2) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,2)*xr(i-1,i) + dot_prod(i-1,2)*xr(i-2,i-1) +$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,2)*xr(i-1,i) +$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,2)*xr(i-2,i-1)) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,2)*xr(i-2,i-1))) $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,2)*yr(i-1,i) + dot_prod(i-1,2)*yr(i-2,i-1) +$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,2)*yr(i-1,i) +$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,2)*yr(i-2,i-1)) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,2)*yr(i-2,i-1))) $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,2)*zr(i-1,i) + dot_prod(i-1,2)*zr(i-2,i-1) +$/;"	l	subroutine:CALC_GRADIENT	file:
