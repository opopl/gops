1	bln.f90	/^     1        dot_prod(i-2,3)*dot_prod(i-1,1))*(dot_prod(i,1)*yr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,3)*dot_prod(i-1,1))*(dot_prod(i,1)*zr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,3)*dot_prod(i-1,1))*(dot_prod(i-2,1)*xr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,3)*dot_prod(i-1,1))*(dot_prod(i-2,1)*yr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-2,3)*dot_prod(i-1,1))*(dot_prod(i-2,1)*zr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-3,3)*dot_prod(i-2,1))*(dot_prod(i-2,1)*xr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-3,3)*dot_prod(i-2,1))*(dot_prod(i-2,1)*yr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
