!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
1	bln.f90	/^     1                             BOND_ANGLE,TOR_ANGLE,RADII,NTYPE)$/;"	l	subroutine:CALC_INT_COORDS	file:
1	bln.f90	/^     1                            radii,ntype)$/;"	l	subroutine:G46MERDIFF	file:
1	bln.f90	/^     1                           BOND_ANGLE,TOR_ANGLE,RADII,NTYPE)$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1                         BOND_ANGLE,TOR_ANGLE,RADII,NTYPE)$/;"	l	subroutine:CALC_ENERGY	file:
1	bln.f90	/^     1                       bond_angle,tor_angle,radii,ntype)$/;"	l	subroutine:CALC_DYN	file:
1	bln.f90	/^     1                       bond_angle,tor_angle,radii,ntype)$/;"	l	subroutine:P46MERDIFF	file:
1	bln.f90	/^     1                      BOND_ANGLE,TOR_ANGLE,RADII,NTYPE)$/;"	l	subroutine:CALC_DYN	file:
1	bln.f90	/^     1                     bond_angle,tor_angle,radii,ntype)$/;"	l	subroutine:CALC_DYN	file:
1	bln.f90	/^     1                     bond_angle,tor_angle,radii,ntype)$/;"	l	subroutine:P46MERDIFF	file:
1	bln.f90	/^     1                   A4, COEF, COEF1, COEF2, COEF3, A3, DEN2, A2, A1, DEN1, RNUM, DEN,$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1                   C_PARAM(N), RK_THETA, RK_R, EPSILON, SIGMA, THETA_0, DELTA, RMASS$/;"	l	subroutine:G46MERDIFF	file:
1	bln.f90	/^     1                   QO(*), ENERGY, S6, E_NBOND, E_BOND, E_BANGLE$/;"	l	subroutine:CALC_ENERGY	file:
1	bln.f90	/^     1                   bond_angle,tor_angle,radii,ntype)$/;"	l	subroutine:P46MERDIFF	file:
1	bln.f90	/^     1                  x(n), y(n), z(n), xr(n,n), yr(n,n), zr(n,n),$/;"	l	subroutine:CALC_ENERGY	file:
1	bln.f90	/^     1                  x(n), y(n), z(n), xr(n,n), yr(n,n), zr(n,n),$/;"	l	subroutine:P46MERDIFF	file:
1	bln.f90	/^     1                  x(n), y(n),z(n),xr(n,n),yr(n,n),zr(n,n),$/;"	l	subroutine:CALC_DYN	file:
1	bln.f90	/^     1                  x(n),y(n),z(n),xr(n,n), yr(n,n), zr(n,n),$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1                bond_angle,tor_angle,radii,ntype)$/;"	l	subroutine:P46MERDIFF	file:
1	bln.f90	/^     1               dot_prod(i,2)*dot_prod(i,2)   $/;"	l	subroutine:CALC_INT_COORDS	file:
1	bln.f90	/^     1               dsqrt(dot_prod(i+1,1)*dot_prod(i,1))$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1              (b_param(i,j)*s6\/(rad7*radii(i,j))))$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1              dsqrt(dot_prod(i,1)*dot_prod(i-1,1))$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1         *dcos(tor_angle(i+1))-3.0))$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1         *dot_prod(i,1))$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1         *dot_prod(i-1,1))$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1         dot_prod(i+1,1)*xr(i+2,i+3) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        ((dot_prod(i-2,2)\/dot_prod(i-1,1))*xr(i-1,i) $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        ((dot_prod(i-2,2)\/dot_prod(i-1,1))*yr(i-1,i) $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        ((dot_prod(i-2,2)\/dot_prod(i-1,1))*zr(i-1,i) $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        (1.0D0\/x_prod(i))*(dot_prod(i+1,2)*dot_prod(i,2) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        (1.0D0\/x_prod(i))*(dot_prod(i,2)*dot_prod(i-1,2) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        (1.0D0\/x_prod(i-1))*(dot_prod(i,2)*dot_prod(i-1,2) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        (1.0D0\/x_prod(i-1))*(dot_prod(i-1,2)*dot_prod(i-2,2) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        (1.0D0\/x_prod(i-2))*(dot_prod(i-1,2)*dot_prod(i-2,2) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        (1.0D0\/x_prod(i-2))*(dot_prod(i-2,2)*dot_prod(i-3,2) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        *(1.0D0\/dsqrt(x_prod(i+1)*x_prod(i)))  $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        *dcos(tor_angle(i)) - $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        *dcos(tor_angle(i)) -3.0))*(1.0D0\/dsqrt(x_prod(i)*x_prod(i-1)))  $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        *dcos(tor_angle(i-1)) - $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        *dcos(tor_angle(i-2)) - $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        *dot_prod(i-1,1))$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        *dsqrt(dot_prod(i+1,1)*dot_prod(i,1))$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        *dsqrt(dot_prod(i,1)*dot_prod(i-1,1))$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        *xr(i-1,i) + xr(i,i+1) - xr(i-1,i))\/den$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        *yr(i-1,i) + yr(i,i+1) - yr(i-1,i))\/den$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        *zr(i-1,i) + zr(i,i+1) - zr(i-1,i))\/den$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        - (1.0D0\/x_prod(i-2))*(dot_prod(i-2,2)*dot_prod(i-3,2) $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        - dot_prod(i-2,1)*xr(i-3,i-2) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        - dot_prod(i-3,3)*dot_prod(i-2,1))*(dot_prod(i-2,1)*yr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        - xr(i-2,i-1))\/den$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        - yr(i-2,i-1))\/den$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        - zr(i-2,i-1))\/den$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        2.0*dot_prod(i-2,3)*xr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        2.0*dot_prod(i-2,3)*yr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        2.0*dot_prod(i-2,3)*zr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        3.0))*(1.0D0\/dsqrt(x_prod(i)*x_prod(i-1)))  $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        3.0))*(1.0D0\/dsqrt(x_prod(i+1)*x_prod(i)))  $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        3.0))*(1.0D0\/dsqrt(x_prod(i-1)*x_prod(i-2)))  $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        3.0))*(1.0D0\/dsqrt(x_prod(i-2)*x_prod(i-3)))  $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i+1,1)*xr(i+2,i+3) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i+1,1)*yr(i+2,i+3) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i+1,1)*zr(i+2,i+3) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,1))*xr(i,i+1) - xr(i+1,i+2))\/den1$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,1))*yr(i,i+1) - yr(i+1,i+2))\/den1$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,1))*zr(i,i+1) - zr(i+1,i+2))\/den1$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,1)*xr(i+1,i+2) + 2.0*dot_prod(i-1,3)*xr(i,i+1) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,1)*yr(i+1,i+2) + 2.0*dot_prod(i-1,3)*yr(i,i+1) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,1)*zr(i+1,i+2) + 2.0*dot_prod(i-1,3)*zr(i,i+1) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,2)*xr(i+1,i+2))) $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,2)*xr(i+1,i+2)))$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,2)*xr(i,i+1) - dot_prod(i,2)*xr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,2)*yr(i+1,i+2))) $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,2)*yr(i+1,i+2)))$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,2)*yr(i,i+1) - dot_prod(i,2)*yr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,2)*zr(i+1,i+2))) $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,2)*zr(i,i+1) - dot_prod(i,2)*zr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,3)*dot_prod(i+1,1))*(-dot_prod(i+1,1)*xr(i,i+1) +$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,3)*dot_prod(i+1,1))*(-dot_prod(i+1,1)*yr(i,i+1) +$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i,3)*dot_prod(i+1,1))*(-dot_prod(i+1,1)*zr(i,i+1) +$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,1))*xr(i-1,i) - xr(i-2,i-1))\/den1$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,1))*xr(i-1,i) - xr(i-2,i-1))\/den2$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,1))*yr(i-1,i) - yr(i-2,i-1))\/den1$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,1))*yr(i-1,i) - yr(i-2,i-1))\/den2$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,1))*zr(i-1,i) - zr(i-2,i-1))\/den1$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,1))*zr(i-1,i) - zr(i-2,i-1))\/den2$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,1)*xr(i,i+1) - dot_prod(i-1,2)*xr(i,i+1) +$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,1)*xr(i-2,i-1) - 2.0*dot_prod(i-2,3)*xr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,1)*yr(i,i+1) - dot_prod(i-1,2)*yr(i,i+1) +$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,1)*yr(i-2,i-1) - 2.0*dot_prod(i-2,3)*yr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,1)*zr(i,i+1) - dot_prod(i-1,2)*zr(i,i+1) +$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,1)*zr(i-2,i-1) - 2.0*dot_prod(i-2,3)*zr(i-1,i) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,2)*xr(i-1,i)) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,2)*xr(i-1,i))) $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,2)*xr(i-2,i-1) +  dot_prod(i-1,1)*xr(i-2,i-1) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,2)*yr(i-1,i)) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,2)*yr(i-1,i))) $/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,2)*yr(i-1,i)))$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,2)*yr(i-2,i-1) +  dot_prod(i-1,1)*yr(i-2,i-1) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,2)*zr(i-1,i)) -$/;"	l	subroutine:CALC_GRADIENT	file:
1	bln.f90	/^     1        dot_prod(i-1,2)*zr(i-1,i))) $/;"	l	subroutine:CALC_GRADIENT	file:
