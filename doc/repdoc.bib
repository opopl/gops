@article{Crooks1999,
  author = {Crooks, G E},
  title = {{Entropy production fluctuation theorem and the nonequilibrium work
	relation for free energy differences.}},
  journal = {Physical review. E, Statistical physics, plasmas, fluids, and related
	interdisciplinary topics},
  year = {1999},
  volume = {60},
  pages = {2721--6},
  number = {3},
  month = sep,
  abstract = {There are only a very few known relations in statistical dynamics
	that are valid for systems driven arbitrarily far-from-equilibrium.
	One of these is the fluctuation theorem, which places conditions
	on the entropy production probability distribution of nonequilibrium
	systems. Another recently discovered far from equilibrium expression
	relates nonequilibrium measurements of the work done on a system
	to equilibrium free energy differences. In this paper, we derive
	a generalized version of the fluctuation theorem for stochastic,
	microscopically reversible dynamics. Invoking this generalized theorem
	provides a succinct proof of the nonequilibrium work relation.},
  file = {:home/op226/doc/papers/ChemPhys/Crooks-PRE-60-2721-1999.pdf:pdf},
  issn = {1063-651X},
  pmid = {11970075},
