@article{Trepagnier04,
  author = {Trepagnier, E H and Jarzynski, C and Ritort, F and Crooks, G E and
	Bustamante, C J and Liphardt, J},
  title = {{Experimental test of Hatano and Sasa's nonequilibrium steady-state
