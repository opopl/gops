@article{Faradjian2004,
  author = {Faradjian, Anton K and Elber, Ron},
  title = {{Computing time scales from reaction coordinates by milestoning.}},
  journal = {The Journal of chemical physics},
  year = {2004},
  volume = {120},
  pages = {10880--9},
  number = {23},
  month = jun,
  abstract = {An algorithm is presented to compute time scales of complex processes
	following predetermined milestones along a reaction coordinate. A
	non-Markovian hopping mechanism is assumed and constructed from underlying
	microscopic dynamics. General analytical analysis, a pedagogical
	example, and numerical solutions of the non-Markovian model are presented.
