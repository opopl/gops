@Article{Marko95,
	Author = "JF Marko and ED Siggia",
	Title = "{Stretching DNA}",
	journal = "{Macromolecules}",
	Year = "{1995}",
	Volume = "{28}",
	Pages = "{8759--8770}",
	Abstract = "{A statistical mechanical treatment of the wormlike chain
model (WLC) is used to analyze experiments in which double-stranded DNA,
tethered at one end, is stretched by a force applied directly to the free end,
by an electric field, or by hydrodynamic flow. All experiments display a
strong-stretching regime where the end-to-end distance approaches the DNA
contour length as 1/(force)(1/2), which is a clear signature of WLC elasticity.
The elastic properties of DNA become scale dependent in the presence of
electrostatic interactions; the effective electric charge and the intrinsic
bending elastic constant are determined from experiments at low salt
concentration. We also consider the effects of spontaneous bends and the
distortion of the double helix by strong forces.}"
}

@article{Flomenbom2006,
  author = {Flomenbom, Ophir and Silbey, Robert J},
  title = {{Utilizing the information content in two-state trajectories.}},
  journal = {Proceedings of the National Academy of Sciences of the United States
	of America},
  year = {2006},
  volume = {103},
  pages = {10907--10},
  number = {29},
  month = jul,
  abstract = {The signal from many single-molecule experiments monitoring molecular
	processes, such as enzyme turnover by means of fluorescence and opening
	and closing of ion channel through the flux of ions, consists of
	a time series of stochastic "on" and "off" (or open and closed) periods,
	termed a two-state trajectory. This signal reflects the dynamics
	in the underlying multisubstate on-off kinetic scheme (KS) of the
	process. The determination of the underlying KS is difficult and
	sometimes even impossible because of the loss of information in the
	mapping of the multidimensional KS onto two dimensions. Here we introduce
	a previously undescribed procedure that efficiently and optimally
	relates the signal to all equivalent underlying KS. This procedure
	partitions the space of KS into canonical (unique) forms that can
	handle any KS and obtains the topology and other details of the canonical
	form from the data without the need for fitting. Also established
	are relationships between the data and the topology of the canonical
	form to the on-off connectivity of a KS. The suggested canonical
	forms constitute a powerful tool in discriminating between KS. Based
	on our approach, the upper bound on the information content in two-state
	trajectories is determined.},
  doi = {10.1073/pnas.0604546103},
  file = {:home/op226/doc/papers/ChemPhys/Flomenbom-PNAS-103-10907-2006.pdf:pdf},
  issn = {0027-8424},
  keywords = {Ion Channel Gating,Ion Channels,Ion Channels: chemistry,Ion Channels:
	metabolism,Models, Biological},
  pmid = {16832051},
  url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1544147\&tool=pmcentrez\&rendertype=abstract}
}

@article{Freund09,
  author = {Freund, L. B.},
  title = {{Characterizing the resistance generated by a molecular bond as it
	is forcibly separated.}},
  journal = {Proceedings of the National Academy of Sciences of the United States
	of America},
  year = {2009},
  volume = {106},
  pages = {8818--23},
  number = {22},
  month = jun,
  abstract = {The goal of measurements of the resisting force generated by a molecular
	bond as it is being forcibly separated under controlled conditions
	is to determine functional characteristics of the bond. Here, we
	establish the dependence of force history during unbinding on both
	those parameters chosen to characterize the bond itself and the controllable
	loading parameters. This is pursued for the practical range of behavior
	in which unbinding occurs diffusively rather than ballistically,
	building on the classic work of Kramers. For a bond represented by
	a one-dimensional energy landscape, modified by a second time-dependent
	energy profile representing applied loading, we present a mathematical
	analysis showing the dependence of the resistance of the bond-on-bond
	well shape, general time dependence of the imposed loading, and stiffness
	of the loading apparatus. The quality of the result is established
	through comparison with full numerical solutions of the underlying
	Smoluchowski equation.},
  doi = {10.1073/pnas.0903003106},
  file = {:home/op226/doc/papers/ChemPhys/Freund-PNAS-106-8818-2009.pdf:pdf},
  issn = {1091-6490},
  pmid = {19416901},
  url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2674939\&tool=pmcentrez\&rendertype=abstract}
}

