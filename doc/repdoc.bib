@Article{Maisuradze09,
  title = {How Adequate are One- and Two-Dimensional Free Energy Landscapes for Protein Folding Dynamics?},
  author = {Maisuradze, Gia G. and Liwo, Adam  and Scheraga, Harold A.},
  journal = {Phys. Rev. Lett.},
  volume = {102},
  number = {23},
  pages = {238102},
  numpages = {4},
  year = {2009},
  month = {Jun},
  doi = {10.1103/PhysRevLett.102.238102},
  publisher = {American Physical Society},
abstract={
The molecular dynamics trajectories of protein folding or unfolding, generated
with the coarse-grained united-residue force field for the B domain of
staphylococcal protein A, were analyzed by principal component analysis (PCA).
The folding or unfolding process was examined by using free-energy landscapes
(FELs) in PC space. By introducing a novel multidimensional FEL, it was shown
that the low-dimensional FELs are not always sufficient for the description of
folding or unfolding processes. Similarities between the topographies of FELs
along low- and high-indexed principal components were observed.  }
}

