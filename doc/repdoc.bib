@Article{Kondov09,
	Author = "Ivan Kondov and Abhinav Verma and Wolfgang Wenzel",
	Title = "{Folding Path and Funnel Scenarios for Two Small Disulfide-Bridged Proteins}",
	Journal = "{BIOCHEMISTRY}",
	Year = "{2009}",
	Volume = "{48}",
	Number = "{34}",
	Pages = "{8195--8205}",
	Month = "{SEP 1}",
	Abstract = "{The presence of disulfide bonds leads to an interesting
interplay between noncovalent intramolecular interactions and disulfide bond
formation even in small proteins. Here we have investigated the Folding
mechanism of the 23-residue potassium channel blocker 1WQE and the 18-residue
antimicrobial peptide protegrin-1 1PG1, as two proteins containing disulfide
bridges, in all-atom basin hopping simulations starting From completely
extended conformations. The minimal-energy conformations deviate by only 2.1
and 1.2 angstrom for 1WQE and 1PG1, respectively, from their structurally
conserved experimental conformations. A detailed analysis or their free energy
surfaces demonstrates that the folding mechanism of disulfide-bridged proteins
can vary dramatically from Levinthal's single-path scenario to it cooperative
process consistent with the funnel paradigm of protein folding.}"
 }

