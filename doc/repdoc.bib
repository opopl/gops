@article{MillerWales99,
keywords={BLN;PES;DG;pathways},
  author = {Miller, Mark A. and Wales, David J.},
  title = {{Energy landscape of a model protein}},
  journal = {The Journal of Chemical Physics},
  year = {1999},
  volume = {111},
  pages = {6610},
  number = {14},
  doi = {10.1063/1.480011},
  file = {:home/op226/doc/papers/ChemPhys/Miller-Wales-JCP-111-6610-1999.pdf:pdf},
  issn = {00219606},
  url = {http://link.aip.org/link/JCPSA6/v111/i14/p6610/s1\&Agg=doi},
  abstract={ The potential energy surface of an off-lattice model protein is
characterized in detail by constructing a disconnectivity graph and by
examining the organization of pathways on the surface. The results clearly
reveal the frustration exhibited by this system and explain why it does not
fold efficiently to the global potential energy minimum. In contrast, when the
frustration is removed by constructing a “Gō-type”. model, the resulting graph
exhibits the characteristics expected of a folding funnel.
  }
}

