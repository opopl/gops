@article{Onoa03,
author = {Onoa, Bibiana and Dumont, Sophie and Liphardt, Jan and Smith, Steven
B. and Tinoco, Ignacio and Bustamante, Carlos}, 
title = {{Identifying Kinetic
Barriers to Mechanical Unfolding of the T. thermophila Ribozyme}}, volume =
{299}, number = {5614}, pages = {1892-1895}, year = {2003}, doi =
{10.1126/science.1081338}, 
abstract ={Mechanical unfolding trajectories for
single molecules of the Tetrahymena thermophila ribozyme display eight
intermediates corresponding to discrete kinetic barriers that oppose mechanical
unfolding with lifetimes of seconds and rupture forces between 10 and 30
piconewtons. Barriers are magnesium dependent and correspond to known intra-
and interdomain interactions. Several barrier structures are “brittle,”
breakage requiring high forces but small (1 to 3 nanometers) deformations.
Barrier crossing is stochastic, leading to variable unfolding paths. The
response of complex RNA structures to locally applied mechanical forces may be
analogous to the responses of RNA during translation, messenger RNA export from
the nucleus, and viral replication.}, URL =
{http://www.sciencemag.org/content/299/5614/1892.abstract}, eprint =
{http://www.sciencemag.org/content/299/5614/1892.full.pdf}, journal = {Science} 
}



@article{FormanClarke07,
