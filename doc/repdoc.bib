@article{DoyeWales95,
author = {Jonathan P. K. Doye and David J. Wales},
collaboration = {},
title = {Calculation of thermodynamic properties of small Lennard-Jones clusters incorporating anharmonicity},
publisher = {AIP},
year = {1995},
journal = {The Journal of Chemical Physics},
volume = {102},
number = {24},
pages = {9659-9672},
keywords = {ATOMIC CLUSTERS; THERMODYNAMICS; ANHARMONICITY; ENERGYLEVEL DENSITY},
