@Article{ Bongini03,
	title = "Thermally activated processes in polymer dynamics",
	author = "Lorenzo Bongini and Roberto Livi and Antonio Politi and Alessandro Torcini",
	journal = "Phys. Rev. E",
	volume = "68",
	number = "6",
	pages = "061111",
	numpages = "12",
	year = "2003",
	month = "Dec",
	doi = "10.1103/PhysRevE.68.061111",
	publisher = "American Physical Society",
	abstract = " Jumps between neighboring minima in the energy landscape of both homopolymeric and heteropolymeric chains are numerically investigated by determining the average escape time from different valleys. The numerical results are compared to the theoretical expression derived by Langer [J.S. Langer, Ann. Phys. (N.Y.) 54, 258 (1969)] with reference to a 2N-dimensional space. Our simulations indicate that the dynamics within the native valley is well described by a sequence of thermally activated process up to temperatures well above the folding temperature. At larger temperatures, systematic deviations from the Langer‚Äôs estimate are instead observed. Several sources for such discrepancies are thoroughly discussed. "
}

@Article{ Torcini01,
	author = "A. Torcini and R. Livi and A. Politi",
	title = "A Dynamical Approach to Protein Folding",
	journal = "Journal of Biological Physics",
	publisher = "Springer Netherlands",
	issn = "0092-0606",
	keyword = "Physics and Astronomy",
	pages = "181--203",
	volume = "27",
	issue = "2",
	url = "http://dx.doi.org/10.1023/A:1013104123892",
	note = "10.1023/A:1013104123892",
	abstract = "In this paper we show that a dynamical description of the protein folding process provides an effective representation of equilibrium properties and it allows for a direct investigation of the mechanisms ruling the approach towards the native configuration. The results reported in this paper have been obtained fora two-dimensional toy-model of aminoacid sequences, whosenative configurations were previously determined byMonte Carlo techniques.The somewhat controversial scenario emerging from the comparison among different thermodynamical indicators is definitely better resolved with the help of a truly dynamical description. In particular,we are able to identify the metastable states visited during the folding process by monitoring the temporal evolution of the `long-range' potentialenergy. Moreover, the resulting dynamical scenario is consistent with the picture arising from a reconstruction of the energy landscape in the vicinity of the global minimum. This suggests that the introduction of efficient `static' indicators too should properly account for the complex `orography' of the landscape.",
	year = "2001"
}

@Article{ Mitternacht2009,
	title = "Changing the Mechanical Unfolding Pathway of FnIII10 by Tuning the Pulling Strength",
	journal = "Biophysical Journal",
	volume = "96",
	number = "2",
	pages = "429--441",
	year = "2009",
	note = "",
	issn = "0006-3495",
	doi = "DOI: 10.1016/j.bpj.2008.09.043",
	url = "http://www.sciencedirect.com/science/article/B94RW-4VF5H8J-H/2/1fe82cb1d5c1692cb67cb491c883ccb9",
	author = {Simon Mitternacht and Stefano Luccioli and Alessandro Torcini and Alberto Imparato and Anders Irb{\"a}ck},
	abstract = " We investigate the mechanical unfolding of the tenth type III domain from fibronectin (FnIII10) both at constant force and at constant pulling velocity, by all-atom Monte Carlo simulations. We observe both apparent two-state unfolding and several unfolding pathways involving one of three major, mutually exclusive intermediate states. All three major intermediates lack two of seven native [beta]-strands, and share a quite similar extension. The unfolding behavior is found to depend strongly on the pulling conditions. In particular, we observe large variations in the relative frequencies of occurrence for the intermediates. At low constant force or low constant velocity, all three major intermediates occur with a significant frequency. At high constant force or high constant velocity, one of them, with the N- and C-terminal [beta]-strands detached, dominates over the other two. Using the extended Jarzynski equality, we also estimate the equilibrium free-energy landscape, calculated as a function of chain extension. The application of a constant pulling force leads to a free-energy profile with three major local minima. Two of these correspond to the native and fully unfolded states, respectively, whereas the third one can be associated with the major unfolding intermediates."
}

