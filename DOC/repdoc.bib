@Article{ Laviolette85,
	author = "Randall A. LaViolette and Frank H. Stillinger",
	collaboration = "",
	title = "Multidimensional geometric aspects of the solid--liquid transition in simple substances",
	publisher = "AIP",
	year = "1985",
	journal = "The Journal of Chemical Physics",
	volume = "83",
	number = "8",
	pages = "4079--4085",
	keywords = "MOLECULAR DYNAMICS CALCULATION, PHASE TRANSFORMATIONS, MELTING, TOPOLOGY, RARE GASES, LIQUID STRUCTURE",
	url = "http://link.aip.org/link/?JCP/83/4079/1",
	doi = "10.1063/1.449826",
	abstract = "Any molecular system explores significantly different regions of the potential‚Äêenergy hypersurface as the system is found, respectively, in the solid and liquid phases. We study in detail the multidimensional geometry of these different regions with molecular‚Äêdynamics calculations for 256 simple atoms in a fixed volume. The atomic interactions are chosen to represent the noble gases. The stable crystal for this model displays a face‚Äêcentered cubic structure. We evaluate the local gradient and curvatures of the regions of the hypersurface sampled by the system for a wide range of temperatures. We observe that a significant fraction of the curvatures become negative in the region sampled by the system at temperatures even as low as one‚Äêfourth the melting temperature. Further, the curvature distribution changes dramatically with respect to temperature at the melting point. We also construct and evaluate a new distribution for the distance between the atoms in their instantaneous dynamical configurations and those in their corresponding ‚Äò‚Äòquenched‚Äô‚Äô configuration (i.e., the configuration found at the corresponding potential‚Äêenergy minimum). With the help of this new distribution, we conclude that the quenched configurations which are encountered during the melting process are structures which contain vacancy‚Äêinterstitial defect pairs. "
}

