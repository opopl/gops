@Article{ Shapiro97,
	author = "B E Shapiro and H Qian",
	title = "{A quantitative analysis of single protein-ligand complex separation with the atomic force microscope.}",
	journal = "Biophysical chemistry",
	year = "1997",
	volume = "67",
	pages = "211--9",
	number = "1-3",
