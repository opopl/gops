@Article{ DoyeWales98,
	title = "Thermodynamics of Global Optimization",
	author = "Jonathan P. K. Doye and David J. Wales",
	journal = "Phys. Rev. Lett.",
	volume = "80",
	number = "7",
	pages = "1357--1360",
	numpages = "3",
	year = "1998",
	month = "Feb",
	doi = "10.1103/PhysRevLett.80.1357",
	publisher = "American Physical Society",
	abstract = " Theoretical design of global optimization algorithms can profitably utilize recent statistical mechanical treatments of potential energy surfaces (PES's). Here we analyze a particular method to explain its success in locating global minima on surfaces with a multiple-funnel structure, where trapping in local minima with different morphologies is expected. We find that a key factor in overcoming trapping is the transformation applied to the PES which broadens the thermodynamic transitions. The global minimum then has a significant probability of occupation at temperatures where the free energy barriers between funnels are surmountable. "
}

