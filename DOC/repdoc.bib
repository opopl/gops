@Article{ MaloneyLacks2006,
	author = "C. E. Maloney and D. J. Lacks",
	title = "Energy Barrier Scalings in Driven Systems",
	journal = "Phys. Rev. E",
	volume = "73",
	pages = "061106",
	year = "2006",
	abstract = " Energy landscape mappings are performed for two different molecular systems under mechanical loads. Barrier heights are observed to scale as ŒîU‚àºŒ¥3‚àï2, where Œ¥ is a residual load. Catastrophe theory predicts that this scaling should arise for vanishing Œ¥; however, this region is irrelevant in physical processes at finite temperature because thermal fluctuations cause the system to cross over the barrier before reaching the small-Œ¥ regime. Surprisingly, we find that the ŒîU‚àºŒ¥3‚àï2 scaling is valid far beyond the vanishing Œ¥ regime described by catastrophe theory. We discuss how this scaling will therefore be relevant at finite temperatures and gives corrections to Eyring‚Äôs theory for transition rates. "
}

@Article{ TshiprutUrbakh09,
	author = "Zion Tshiprut and Michael Urbakh",
	collaboration = "",
	title = "Exploring hysteresis and energy dissipation in single-molecule force spectroscopy",
	publisher = "AIP",
	year = "2009",
	journal = "The Journal of Chemical Physics",
	volume = "130",
	number = "8",
	eid = "084703",
	numpages = "8",
	pages = "084703",
	keywords = "hysteresis, potential energy functions",
	url = "http://link.aip.org/link/?JCP/130/084703/1",
	doi = "10.1063/1.3077867",
	abstract = " We propose an analytical approach to describe the active rebinding and force hysteresis observed in single-molecule pulling experiments. We derive equations for dependences of the measured quantities on the properties of molecular potential, effective stiffness of the pulling spring, and the pulling velocity. The calculations predict that the energy dissipated per an unbinding-rebinding cycle strongly increases with the steepness of the molecular potential and with decreasing the spring stiffness. A comparison of analytical results with Langevin simulations shows that the scaling relations for the barrier heights and most probable forces are more accurate in the case of active rebinding than for unbinding. Our consideration demonstrates that simultaneous analysis of probability density functions for unbinding and rebinding forces improves essentially the accuracy of retrieval information on intrinsic parameters of the molecular complex from the force measurements. "
}

