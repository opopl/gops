@Article{ Gregurick96,
	Author = "SK Gregurick and M. H. Alexander and B Hartke",
	Title = "{Global geometry optimization of (Ar)(n) and B(Ar)(n) clusters using a modified genetic algorithm}",
	Journal = "{JOURNAL OF CHEMICAL PHYSICS}",
