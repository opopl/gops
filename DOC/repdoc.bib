	ISSN = "{0028-0836}",
	Keywords-Plus = "{GLOBULAR-PROTEINS; DYNAMICS}",
	Subject-Category = "{Multidisciplinary Sciences}",
	Number-of-Cited-References = "{22}",
	Times-Cited = "{666}",
	Journal-ISO = "{Nature}",
	Doc-Delivery-Number = "{NM067}",
