@Article{ Flomenbom2006,
	author = "Ophir Flomenbom and Robert J Silbey",
	title = "{Utilizing the information content in two-state trajectories.}",
	journal = "Proceedings of the National Academy of Sciences of the United States of America",
	year = "2006",
	volume = "103",
	pages = "10907--10",
	number = "29",
	month = jul,
	abstract = {The signal from many single-molecule experiments monitoring molecular processes, such as enzyme turnover by means of fluorescence and opening and closing of ion channel through the flux of ions, consists of a time series of stochastic "on" and "off" (or open and closed) periods, termed a two-state trajectory. This signal reflects the dynamics in the underlying multisubstate on-off kinetic scheme (KS) of the process. The determination of the underlying KS is difficult and sometimes even impossible because of the loss of information in the mapping of the multidimensional KS onto two dimensions. Here we introduce a previously undescribed procedure that efficiently and optimally relates the signal to all equivalent underlying KS. This procedure partitions the space of KS into canonical (unique) forms that can handle any KS and obtains the topology and other details of the canonical form from the data without the need for fitting. Also established are relationships between the data and the topology of the canonical form to the on-off connectivity of a KS. The suggested canonical forms constitute a powerful tool in discriminating between KS. Based on our approach, the upper bound on the information content in two-state trajectories is determined.},
	doi = "10.1073/pnas.0604546103",
	file = ":home/op226/doc/papers/ChemPhys/Flomenbom-PNAS-103-10907-2006.pdf:pdf",
	issn = "0027-8424",
	keywords = "Ion Channel Gating, Ion Channels, Ion Channels: chemistry, Ion Channels: metabolism, Models, Biological",
	pmid = "16832051",
	url = "http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1544147&tool=pmcentrez&rendertype=abstract"
}

@Article{ Freund09,
	author = "L. B. Freund",
