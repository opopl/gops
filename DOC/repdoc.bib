	ISSN = "{0269-2139}",
	Keywords = "{INTRACTABILITY, NP-COMPLETENESS, PEPTIDE BACKBONE CONFORMATION, POTENTIAL ENERGY MINIMIZATION, PROTEIN STRUCTURE PREDICTION}",
	Subject-Category = "{Biochemistry \& Molecular Biology; Biotechnology \& Applied Microbiology}",
