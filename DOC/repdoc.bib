@Article{ Liphardt2002,
	author = "Jan Liphardt and Sophie Dumont and Steven B. Smith and Ignacio Tinoco and Carlos J. Bustamante",
	title = "{Equilibrium information from nonequilibrium measurements in an experimental test of Jarzynski's equality.}",
