@Article{ Marsiglio87,
	title = "On spinodals and catastrophes",
	journal = "Physics Letters A",
	volume = "123",
	number = "2",
	pages = "79--81",
	year = "1987",
	note = "",
	issn = "0375-9601",
	doi = "DOI: 10.1016/0375-9601(87)90660-8",
	url = "http://www.sciencedirect.com/science/article/B6TVM-46V0M8P-4D/2/e298a171bc6de83bd9fac3c4c46a638b",
	author = "F. Marsiglio and F. D. Manchester",
	abstract = " A spinodal is the locus of points satisfying a stability limit criterion, and as such, invites comparison with the description of instabilities given by catastrophe theory. We give a discussion concerning the identification of a spinodal with a specific member of the family of elementary catastrophes and show that the cusp catastrophe is the relevant member, a result at variance with an earlier identification. In comparing a cusp catastrophe with the thermodynamics of phase transitions, the comaprison should be limited to those systems which clearly satisfy mean field theories and display a well defined spinodal; hydrogen-in-metal systems provide the best {\'e}xperimental examples, found so far, of such systems."
}

