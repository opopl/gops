@Article{ ToanThirumalai10,
	author = "Ngo Minh Toan and Devarajan Thirumalai",
	title = "Theory of Biopolymer Stretching at High Forces",
	journal = "Macromolecules",
	volume = "43",
	number = "9",
	pages = "4394--4400",
	year = "2010",
	doi = "10.1021/ma902008y",
	URL = "http://pubs.acs.org/doi/abs/10.1021/ma902008y",
	eprint = "http://pubs.acs.org/doi/pdf/10.1021/ma902008y",
	abstract = " We provide a unified theory for the high force entropic elasticity of biopolymers solely in terms of the persistence length, Œæp, and the monomer spacing, a. When the force f > ‚Ñ±h ‚àº kBTŒæp/a2 the biopolymers behave as freely jointed chains (FJCs) while in the range ‚Ñ±l ‚àº kBT/Œæp < f < ‚Ñ±h the worm-like chain (WLC) is a better model. We show that Œæp can be estimated from the force extension curve (FEC) at the extension x ‚âà 1/2 (normalized by the contour length of the biopolymer). After validating the theory using simulations, we provide a quantitative analysis of the FECs for a diverse set of biopolymers (dsDNA, ssRNA, ssDNA, polysaccharides, and unstructured PEVK domain of titin) for x ‚â• 1/2. The success of a specific polymer model (FJC or WLC) to describe the FEC of a given biopolymer is naturally explained by the theory. Only by probing the response of biopolymers over a wide range of forces can the f-dependent elasticity be fully described. "
}

@Article{ DobryninCR10,
	author = "Andrey V. Dobrynin and Jan-Michael Y. Carrillo and Michael Rubinstein",
	title = "Chains Are More Flexible Under Tension",
	journal = "Macromolecules",
	volume = "43",
	number = "21",
	pages = "9181--9190",
	year = "2010",
	doi = "10.1021/ma101860t",
	URL = "http://pubs.acs.org/doi/abs/10.1021/ma101860t",
	eprint = "http://pubs.acs.org/doi/pdf/10.1021/ma101860t",
	abstract = " The mechanical response of networks, gels, and brush layers is a manifestation of the elastic properties of the individual macromolecules. Furthermore, the elastic response of macromolecules to an applied force is the foundation of the single-molecule force spectroscopy techniques. The two main classes of models describing chain elasticity include the worm-like and freely jointed chain models. The selection between these two classes of models is based on the assumptions about chain flexibility. In many experimental situations, the choice is not clear, and a model describing the crossover between these two limiting classes is therefore in high demand. We are proposing a unified chain deformation model that describes the force‚àídeformation curve in terms of the chain bending constant, K, and bond length, b. This model demonstrates that the worm-like and freely jointed chain models correspond to two different regimes of polymer deformation, and the crossover between these two regimes depends on the chain bending rigidity and the magnitude of the applied force. Polymer chains with bending constant K > 1 behave as a worm-like chain under tension in the interval of the applied forces f ‚â§ KkBT/b and as a freely jointed chain for f ‚â• KkBT/b. (kB is the Boltzmann constant and T is the absolute temperature.) The proposed crossover expression for chain deformation is in excellent agreement with the results of the molecular dynamics simulations of chain deformation and single-molecule deformation experiments of biological and synthetic macromolecules. "
}

@Article{ Thirumalai10,
	author = "Devarajan Thirumalai and Edward P. O'Brien and Greg Morrison and Changbong Hyeon",
	title = "Theoretical Perspectives on Protein Folding",
	journal = "Annual Review of Biophysics",
	volume = "39",
	number = "1",
	pages = "159--183",
	year = "2010",
	doi = "10.1146/annurev-biophys-051309-103835",
	URL = "http://www.annualreviews.org/doi/abs/10.1146/annurev-biophys-051309-103835",
	eprint = "http://www.annualreviews.org/doi/pdf/10.1146/annurev-biophys-051309-103835",
	abstract = " Understanding how monomeric proteins fold under in vitro conditions is crucial to describing their functions in the cellular context. Significant advances in theory and experiments have resulted in a conceptual framework for describing the folding mechanisms of globular proteins. The sizes of proteins in the denatured and folded states, cooperativity of the folding transition, dispersions in the melting temperatures at the residue level, and timescales of folding are, to a large extent, determined by N, the number of residues. The intricate details of folding as a function of denaturant concentration can be predicted by using a novel coarse-grained molecular transfer model. By watching one molecule fold at a time, using single-molecule methods, investigators have established the validity of the theoretically anticipated heterogeneity in the folding routes and the N-dependent timescales for the three stages in the approach to the native state. Despite the successes of theory, of which only a few examples are documented here, we conclude that much remains to be done to solve the protein folding problem in the broadest sense. "
}

