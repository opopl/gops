	ISSN = "{0028-0836}",
	Keywords-Plus = "{CARBON CLUSTERS; MOLECULAR-DYNAMICS; C-60; FULLERENES; LIQUIDS; SURFACES; MODEL; FRAGMENTATION; SIMULATIONS; RELAXATION}",
